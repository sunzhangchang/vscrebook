{
  "name": "vscrebook",
  "displayName": "vscrebook",
  "description": "VSCode 摸鱼阅读器",
  "version": "0.5.1",
  "engines": {
    "vscode": "^1.63.1"
  },
  "icon": "images/icon.png",
  "categories": [
    "Other"
  ],
  "files": [
    "./pkg/crawl/index.js",
    "./pkg/crawl/index.d.ts"
  ],
  "keywords": [
    "txt",
    "novel",
    "book",
    "小说",
    "神器",
    "摸鱼",
    "划水"
  ],
  "publisher": "Cyanogenaq",
  "activationEvents": [
    "onCommand:vscrebook.start"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "title": "Vscre Book",
      "type": "object",
      "properties": {
        "vscrebook.pageSize": {
          "type": "number",
          "default": 25,
          "description": "每页文字数量 & Number of text per page"
        },
        "vscrebook.downloadPath": {
          "type": "string",
          "default": "",
          "description": "下载的小说的储存路径"
        },
        "vscrebook.autoFlipTime": {
          "type": "number",
          "default": "3000",
          "description": "自动翻页的速度(每页的时间/ms)"
        }
      }
    },
    "commands": [
      {
        "command": "vscrebook.bossKey",
        "title": "bossKey"
      },
      {
        "command": "vscrebook.prevPage",
        "title": "prevPage"
      },
      {
        "command": "vscrebook.nextPage",
        "title": "nextPage"
      },
      {
        "command": "vscrebook.jumpPage",
        "title": "jumpPage"
      },
      {
        "command": "vscrebook.start",
        "title": "start 开始阅读(摸鱼)"
      },
      {
        "command": "vscrebook.autoFlip",
        "title": "autoFlip"
      }
    ],
    "keybindings": [
      {
        "command": "vscrebook.bossKey",
        "key": "ctrl+alt+/",
        "mac": "cmd+/"
      },
      {
        "command": "vscrebook.prevPage",
        "key": "ctrl+alt+,",
        "mac": "cmd+,",
        "when": "editorTextFocus"
      },
      {
        "command": "vscrebook.nextPage",
        "key": "ctrl+alt+.",
        "mac": "cmd+.",
        "when": "editorTextFocus"
      },
      {
        "command": "vscrebook.jumpPage",
        "key": "ctrl+alt+'",
        "mac": "cmd+'",
        "when": "editorTextFocus"
      },
      {
        "command": "vscrebook.start",
        "key": "ctrl+alt+;",
        "mac": "cmd+;"
      },
      {
        "command": "vscrebook.autoFlip",
        "key": "ctrl+alt+m",
        "mac": "cmd+m"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "gulp dev",
    "watch": "gulp watch",
    "package": "gulp prod",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p -w . --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@swc/core": "^1.2.127",
    "@swc/helpers": "^0.3.2",
    "@types/lodash": "^4.14.178",
    "@types/node": "~14.18.5",
    "@types/vscode": "^1.63.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vscode/test-electron": "^1.6.2",
    "core-js": "^3.20.2",
    "esbuild": "^0.14.14",
    "esbuild-loader": "^2.18.0",
    "eslint": "^7.32.0",
    "gulp": "^4.0.2",
    "gulp-util": "^3.0.8",
    "node-loader": "^2.0.0",
    "regenerator-runtime": "^0.13.9",
    "swc-loader": "^0.1.15",
    "typescript": "4.4.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "bugs": {
    "url": "https://github.com/sunzhangchang/vscrebook/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sunzhangchang/vscrebook"
  },
  "homepage": "https://github.com/sunzhangchang/vscrebook/blob/main/README.md",
  "dependencies": {
    "chardet": "^1.4.0",
    "crawl": "file:./pkg/crawl",
    "iconv-lite": "^0.6.3",
    "lodash": "^4.17.21"
  }
}
